@namespace Masa.Docs.Shared.Components
@inject I18n I18n

<AppTooltipButton Href="https://github.com/masastack/Masa.Blazor"
                  Target="_blank"
                  Icon="mdi-github"
                  Path="github"
                  Class="mr-3 ml-2">
</AppTooltipButton>

<AppTooltipButton Href="https://gitee.com/blazorcomponent/MASA.Blazor"
                  Target="_blank"
                  Image="https://cdn.masastack.com/stack/images/website/masa-blazor/gitee.png"
                  Path="gitee"
                  Class="mr-3 ml-2">
</AppTooltipButton>

<AppTooltipButton OnClick="OnSettingsClick"
