        Src="https://cdn.masastack.com/stack/images/website/masa-blazor/carousel/squirrel.jpg">
    </MCarouselItem>

    <MCarouselItem
        Src="https://cdn.masastack.com/stack/images/website/masa-blazor/carousel/sky.jpg">
    </MCarouselItem>

    <MCarouselItem
        Src="https://cdn.masastack.com/stack/images/website/masa-blazor/carousel/bird.jpg">
