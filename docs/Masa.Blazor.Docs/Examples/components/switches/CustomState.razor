<MContainer Fluid>
    <MSwitch
        @bind-Value="switch1"
        TrueValue='"Yes"'
        FalseValue='"No"'
        Label="@($"Switch 1: {switch1}")">
    </MSwitch>
    <MSwitch
        @bind-Value="switch2"
        TrueValue="1"
        FalseValue="0"
        Label="@($"Switch 2: {switch2.ToString()}")">
    </MSwitch>
</MContainer>

@code {

    string switch1 = "Yes";
    int switch2 = 1;

}
