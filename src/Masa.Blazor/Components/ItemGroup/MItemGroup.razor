@namespace Masa.Blazor
@inherits Masa.Blazor.Components.ItemGroup.MItemGroupBase

<CascadingValue Value="IsDark" Name="IsDark">
    <CascadingValue Value="this" IsFixed>
        <div class="@GetClass()"
             style="@GetStyle()"
             id="@Id"
             @ref="Ref"
             @attributes="@Attributes">
            @ChildContent
        </div>
    </CascadingValue>
</CascadingValue>