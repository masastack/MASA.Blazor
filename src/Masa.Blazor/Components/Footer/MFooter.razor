@namespace Masa.Blazor
@inherits ThemeComponentBase

<CascadingValue Value="ComputedTheme" Name="MasaBlazorCascadingTheme">
    <footer id="@Id"
            class="@GetClass()"
            style="@GetStyle()"
            @ref="Ref"
            @attributes="@Attributes">
        @ChildContent
    </footer>
</CascadingValue>