@namespace Masa.Blazor.Presets

<MWindow Value="@_currentAbsolutePath">
    @foreach (var path in _absolutePaths)
    {
        <MWindowItem Transition="" Value="@path" @key="path">
            <PWindowContent Value="@(path == "/app/todo")">
                @ChildContent
            </PWindowContent>
        </MWindowItem>
    }
</MWindow>
