@namespace Masa.Blazor.Presets
@inherits PatternPathComponentBase

<MWindow Value="@_currentAbsolutePath?.Pattern">
    @foreach (var path in PatternPaths)
    {
        <MWindowItem Transition="" Value="@path.Pattern" @key="path">
            <PWindowContent Value="@path.IsSelf">
                @ChildContent
            </PWindowContent>
        </MWindowItem>
    }
</MWindow>
