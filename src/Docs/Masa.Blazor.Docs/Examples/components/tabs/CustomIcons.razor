<MSheet Elevation="6">
    <MTabs BackgroundColor="cyan"
           Dark 
           NextIcon="mdi-arrow-right-bold-box-outline"
           PrevIcon="mdi-arrow-left-bold-box-outline"
           ShowArrows="@true"
           SliderColor="yellow">
        @foreach (var i in Enumerable.Range(1, 30))
        {
            <MTab>Item @i</MTab>
        }
    </MTabs>
</MSheet>