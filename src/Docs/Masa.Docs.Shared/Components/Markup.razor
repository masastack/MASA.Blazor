@namespace Masa.Docs.Shared.Components

<AppCode Class="my-0" Rounded="Rounded">
    <MHighlight Code="@Code" Language="@Language" IgnorePreCssOfTheme></MHighlight>
</AppCode>

@code {

    [Parameter]
    [EditorRequired]
    public string? Code { get; set; }

    [Parameter]
    [EditorRequired]
    public string? Language { get; set; }

    [Parameter]
    public StringBoolean Rounded { get; set; } = true;

}
