@page "/"
@using BlazorComponent.Components
@layout BaseLayout

<div class="home">
    <MRow Class="banner" NoGutters Align="AlignTypes.Center" Justify="JustifyTypes.Center">
        <MCol Align="@AlignTypes.Center" Lg=6 Md=6 Sm=12>
            <div class="banner-logo-box">
                <p class="banner-logo-font">
                    MASA Blazor
                </p>
            </div>
            <div class="banner-detail-box">
                <p class="banner-detail-font">
                    @T("MASABlazorDescription")
                </p>
            </div>
            <MRow NoGutters Style="margin-top:88px;" Justify="JustifyTypes.Start">
                <MButton Dark
                         Color="#5C2D91"
                         Height="64"
                         Class="text-20 mb-6 px-6"
                         Href="getting-started/installation">
                    @T("GetStarted")
                </MButton>
                <MButton Dark
                         Outlined
                         Color="black"
                         Height="64"
                         Class="text-20 px-6 mb-6 mx-6"
                         Href="introduction/why">
                    @T("WhyUseMASABlazor")
                </MButton>
                @*<MButton Dark
                         Color="black"
                         Height="64"
                         Href="https://github.com/BlazorComponent/Masa.Blazor"
                         Target="_blank"
                         Class="text-20 px-6 mb-6">
                    <MIcon Left>mdi-github</MIcon>
                    GitHub
                </MButton>*@
            </MRow>
        </MCol>
        <MCol Align="@AlignTypes.Center" Class="pr-6 d-flex justify-end" Lg=6 Md=6 Sm=12>
            @*We just remove window-item now and will recover it when item-group been refactored*@
            <MWindow>
                <div>
                    <MCard Class="d-flex align-end rounded-0" Color="#F2F3F5" MinHeight="262" MinWidth="262" Width="644" Height="644">
                        <MImage Src="https://cdn.masastack.com/stack/images/website/masa-blazor/banner.png" />
                    </MCard>
                </div>
            </MWindow>
            @if (_length > 1)
            {
                <MItemGroup @bind-Value="OnBoarding"
                        Class="text-center d-flex align-end"
                        Mandatory>
                    @foreach (var item in Enumerable.Range(0, _length))
                    {
                        <MItem Value="item">
                            @if (context.Active)
                            {
                                <img class="mx-1" src="https://cdn.masastack.com/stack/images/website/masa-blazor/SelectPoint.png" />
                            }
                            else
                            {
                                <img style="cursor:pointer;" class="mx-1" src="https://cdn.masastack.com/stack/images/website/masa-blazor/Point.png" @onclick="context.Toggle" />
                            }
                        </MItem>
                    }
                </MItemGroup>
            }
        </MCol>
    </MRow>

    <div class="app-banner" style="display:none;">
        <div class="banner-logo-box">
            <p class="banner-logo-font">
                MASA Blazor
            </p>
        </div>
        <div class="banner-detail-box">
            <p class="banner-detail-font">
                @T("MASABlazorDescription")
            </p>
        </div>
        <div class="d-flex justify-center">
            <MWindow @bind-Value="OnBoarding">
                @foreach (var item in Enumerable.Range(0, _length))
                {
                    <MWindowItem Value="item">
                        <div class="d-flex align-end rounded-0" style="height: 262px;width: 262px;background-color:#F2F3F5;border-color:#F2F3F5">
                            <MImage Src="https://cdn.masastack.com/stack/images/website/masa-blazor/banner.png" />
                        </div>
                    </MWindowItem>
                }
            </MWindow>
            @if (_length > 1)
            {
                <MItemGroup @bind-Value="OnBoarding"
                        Class="text-center d-flex align-end"
                        Mandatory>
                    @foreach (var item in Enumerable.Range(0, _length))
                    {
                        <MItem Value="item">
                            @if (context.Active)
                            {
                                <img style="width:14px;height:3.5px; margin:0 2px;" src="https://cdn.masastack.com/stack/images/website/masa-blazor/SelectPoint.png" />
                            }
                            else
                            {
                                <img style="cursor:pointer; width:3.5px;height:3.5px; margin:0 2px;" src="https://cdn.masastack.com/stack/images/website/masa-blazor/Point.png" @onclick="context.Toggle" />
                            }
                        </MItem>
                    }
                </MItemGroup>
            }
        </div>
        <div class="mt-6">
            <MRow NoGutters Class="mt-6" Justify="JustifyTypes.Center">
                <MButton Dark
                         Width="130"
                         Color="#5C2D91"
                         Height="44"
                         Class="text-14 rounded-lg mr-3"
                         Href="getting-started/installation">
                    @T("GetStarted")
                </MButton>
                @*<MButton Dark
                         Color="black"
                         Width="130"
                         Height="44"
                         Href="https://github.com/BlazorComponent/Masa.Blazor"
                         Target="_blank"
                         Class="rounded-lg text-14 ml-3">
                    <MIcon Left>mdi-github</MIcon>
                    GitHub
                </MButton>*@
                <MButton Dark
                         Outlined
                         Color="black"
                         Height="44"
                         Class="text-14 rounded-lg"
                         Href="introduction/why">
                    @T("WhyUseMASABlazor")
                </MButton>
            </MRow>
            @*<MRow NoGutters Class="mt-6" Justify="JustifyTypes.Center">
                <MButton Dark
                         Outlined
                         Color="black"
                         Width="285"
                         Height="44"
                         Class="text-14 rounded-lg"
                         Href="introduction/why">
                    @T("WhyUseMASABlazor")
                </MButton>
            </MRow>*@
        </div>
    </div>

    <div class="content">
        <div class="video">
            <div class="information">
                <MRow NoGutters>
                    <MCol Align="AlignTypes.Center">
                        <MBorder Color="black" Class="box-title-border rounded-b-xl">
                            <p class="box-title">@T("VideoTeaching")</p>
                        </MBorder>
                    </MCol>
                    <MCol Class="d-flex justify-end" Align="AlignTypes.Center">
                    </MCol>
                </MRow>
            </div>

            <MRow NoGutters>
                <MCol Cols="12" Lg="6" Md="6" Sm="12">
                    <video src="https://cdn.masastack.com/stack/images/website/masa-blazor/video.mp4" controls="controls">
                        your browser does not support the video tag
                    </video>
                </MCol>
                <MCol Cols="12" Lg="6" Md="6" Sm="12" Align="@AlignTypes.Start">
                    <div class="video-description">
                        <div class="video-p">@T("VideoTitle")</div>
                        <div class="video-span">@T("VideoDesc")</div>
                    </div>
                </MCol>
            </MRow>

            <div class="video-group">
                <MRow NoGutters>
                    <MCol Lg="3" Md="6" Sm="12" Class="pa-3">
                        <MHover Context="hoverContext">
                            <MCard Color="#F7F8FA"
                                   @attributes="hoverContext.Attrs"
                                   Class="rounded-lg"
                                   OnClick="@(() => Toggle("https://docs.microsoft.com/zh-cn/aspnet/core/blazor/?view=aspnetcore-6.0"))"
                                   Elevation="@(hoverContext.Hover ? 5 : 0)">
                                <MImage Src="@($"https://cdn.masastack.com/stack/images/website/masa-blazor/info1.png")" Height="288">
                                </MImage>
                                <MCardText Class="pt-4 pl-4 fs-18 text-truncate">
                                    @T("Introduction")
                                </MCardText>
                            </MCard>
                        </MHover>
                    </MCol>
                    <MCol Lg="3" Md="6" Sm="12" Class="pa-3">
                        <MHover Context="hoverContext">
                            <MCard Color="#F7F8FA"
                                   @attributes="hoverContext.Attrs"
                                   Class="rounded-lg"
                                   OnClick="@(() => Toggle("https://docs.microsoft.com/zh-cn/events/dotnetconf-2021/new-blazor-webassembly-capabilities-in-net-6"))"
                                   Elevation="@(hoverContext.Hover ? 5 : 0)">
                                <MImage Src="@($"https://cdn.masastack.com/stack/images/website/masa-blazor/info2.png")" Height="288">
                                </MImage>
                                <MCardText Class="pt-4 pl-4 fs-18 text-truncate">
                                    @T("BlazorWebAssembly")
                                </MCardText>
                            </MCard>
                        </MHover>
                    </MCol>
                    <MCol Lg="3" Md="6" Sm="12" Class="pa-3">
                        <MHover Context="hoverContext">
                            <MCard Color="#F7F8FA"
                                   @attributes="hoverContext.Attrs"
                                   Class="rounded-lg"
                                   OnClick="@(() => Toggle("https://www.dotnetconf.net/agenda"))"
                                   Elevation="@(hoverContext.Hover ? 5 : 0)">
                                <MImage Src="@($"https://cdn.masastack.com/stack/images/website/masa-blazor/info3.png")" Height="288">
                                </MImage>
                                <MCardText Class="pt-4 pl-4 fs-18 text-truncate">
                                    @T("Conf")
                                </MCardText>
                            </MCard>
                        </MHover>
                    </MCol>
                    <MCol Lg="3" Md="6" Sm="12" Class="pa-3">
                        <MHover Context="hoverContext">
                            <MCard Color="#F7F8FA"
                                   @attributes="hoverContext.Attrs"
                                   Class="rounded-lg"
                                   OnClick="@(() => Toggle("https://docs.microsoft.com/zh-cn/aspnet/core/blazor/?view=aspnetcore-6.0"))"
                                   Elevation="@(hoverContext.Hover ? 5 : 0)">
                                <MImage Src="@($"https://cdn.masastack.com/stack/images/website/masa-blazor/info1.png")" Height="288">
                                </MImage>
                                <MCardText Class="pt-4 pl-4 fs-18 text-truncate">
                                    @T("Introduction")
                                </MCardText>
                            </MCard>
                        </MHover>
                    </MCol>
                </MRow>
            </div>
        </div>

