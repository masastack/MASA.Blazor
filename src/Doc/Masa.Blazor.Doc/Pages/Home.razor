        <div class="contributor">
            <div class="information">
                <MRow>
                    <MCol Align="AlignTypes.Center">
                        <MBorder Color="black" Class="box-title-border rounded-b-xl">
                            <p class="box-title">@T("Contributors")</p>
                        </MBorder>
                    </MCol>
                    <MCol Class="d-flex justify-end" Align="AlignTypes.Center">
                    </MCol>
                </MRow>
            </div>
            <div class="contribution">
                <MRow NoGutters Align="AlignTypes.End" Justify="JustifyTypes.Center" Style="font-size: 24px; color: #4E5969;">
                    <p>— @T("ContributorsDesc") —</p>
                </MRow>
                <MRow Justify="JustifyTypes.Center">
                    @for (var i = 0; i < 5; i++)
                    {
                        var index = i + 1;
                        <MCol>
                            <MAvatar Size=77>
                                <MImage Src=@($"https://cdn.masastack.com/stack/images/website/masa-blazor/avatar{index}.png")></MImage>
                            </MAvatar>
                        </MCol>
                    }
                </MRow>
            </div>
        </div>

        <div class="thanks">
            <MRow NoGutters Justify="JustifyTypes.Center" Align="AlignTypes.End" Style="font-size: 24px; color: #4E5969;">
                <p>— @T("Thanks") —</p>
            </MRow>
            <MRow Justify="JustifyTypes.Center">
                <MCol>
                    <img src="https://cdn.masastack.com/stack/images/website/masa-blazor/Blazor.png" />
                </MCol>
                <MCol>
