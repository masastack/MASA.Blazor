                    </MCol>
                    <MCol Lg="4" Md="6" Sm="12" Class="pa-3">
                        <MCard Width="470" Height="470" Color="#F9FAFA" Elevation="0">
                            <div class="why-masa-blazor-card-content fill-height">
                                <div class="d-flex align-center">
                                    <img class="mt-4" src="https://cdn.masastack.com/stack/images/website/masa-blazor/feature4.png">
                                </div>
                                <div class="desc">
                                    <p class="why-masa-blazor-p">@T("EasyToUse")</p>
                                    <p class="why-masa-blazor-span">@T("EasyToUseDesc")</p>
                                </div>
                            </div>
                        </MCard>
                    </MCol>
                    <MCol Lg="4" Md="6" Sm="12" Class="pa-3">
                        <MCard Width="470" Height="470" Color="#F9FAFA" Elevation="0">
                            <div class="why-masa-blazor-card-content fill-height">
                                <div class="d-flex align-center">
                                    <img class="mt-4" src="https://cdn.masastack.com/stack/images/website/masa-blazor/feature5.png">
                                </div>
                                <div class="desc">
                                    <p class="why-masa-blazor-p">@T("CommunityActive")</p>
                                    <p class="why-masa-blazor-span">@T("CommunityActiveDesc")</p>
